{
  "name": "numbers-to-words-i18n",
  "version": "1.1.0",
  "description": "Convert numbers to words in multiple languages (English, French, Arabic)",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "type": "module",
  "scripts": {
    "build": "tsdown",
    "test": "jest",
    "prepublish": "npm run build",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/roniceyemeli/numbers-to-words-i18n.git"
  },
  "bugs": {
    "url": "https://github.com/roniceyemeli/numbers-to-words-i18n/issues"
  },
  "homepage": "https://github.com/roniceyemeli/numbers-to-words-i18n#readme",
  "keywords": [
    "numbers",
    "words",
    "converter",
    "i18n",
    "typescript",
    "english",
    "french",
    "arabic"
  ],
  "author": "roniceyemeli",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "tsdown": "^0.15.5",
    "typescript": "^5.9.2"
  },
  "packageManager": "yarn@4.9.2+sha512.1fc009bc09d13cfd0e19efa44cbfc2b9cf6ca61482725eb35bbc5e257e093ebf4130db6dfe15d604ff4b79efd8e1e8e99b25fa7d0a6197c9f9826358d4d65c3c"
}
